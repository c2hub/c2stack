module tester;

import stdio as io;
import c2stack as stck local;

stack test;

public func int32 main()
{
	stck.init(&test);
	stck.push(&test,"Wordlings!");
	stck.push(&test,", ");
	stck.push(&test,"Hello");
	
	int32 i = total(&test);
	io.printf("total: %d\n", i);
	while(i --> 0)
	{
		io.printf("%s", pop(&test));
	}
	io.puts("\n");
	return 0;
}